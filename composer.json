    "php",
    "security",
    "cipher",
    "encryption",
    "decryption",
    "hash",
    "hashing",
    "mac"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Bahador Mirzazadeh",
      "email": "baha2r.mirzazadeh98@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2.25",
    "ext-openssl": "*",
    "ext-json": "*",
    "ext-redis": "*",
    "symfony/console": "^4.3",
    "baha2rmirzazadeh/phpscanner": "^1.5"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "PHPGuard\\": "PHPGuard/"
    }
